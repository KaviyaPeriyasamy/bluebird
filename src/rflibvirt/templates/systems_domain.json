{
  "Type": "ComputerSystem.0.9.7",
  "Description": "Computer System View",
  "Power": "{{ domain.isActive() }}",
  "HostCorrelation": {
    "HostName": "{{ server.get_fqdn() }}",
    "IPAddress": [
      "{{ server.get_ip() }}"
    ]
  },
  "links": {
    "self": {
      "href": "/redfish/v1/Systems/{{ domain.name() }}"
    }
  },
  "Name": "Computer System",
  "AvailableActions": [
    {
      "Action": "Reset",
      "Capabilities": [
        {
          "AllowableValues": [
            "start",
            "reboot",
            "shutdown",
            "destroy"
          ],
          "PropertyName": "ResetType"
        }
      ]
    }
  ],
  "SystemType": "Virtual"
}

